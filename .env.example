# Kamal Secrets Configuration
# Copy this file to .env and fill in your actual values
# DO NOT commit .env to version control!

# Docker Registry Password (Docker Hub)
KAMAL_REGISTRY_PASSWORD=your_dockerhub_password_or_token

# PostgreSQL Database Password
APP_DATABASE_PASSWORD=your_secure_database_password
POSTGRES_PASSWORD=your_secure_database_password

# JWT Secret Key (use a long random string)
# Generate with: openssl rand -base64 64
APP_JWT_SECRET=your_jwt_secret_key_minimum_32_characters_long
